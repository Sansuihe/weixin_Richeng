<!--pages/details/details.wxml-->
<van-swipe-cell id="swipe-cell2" b right-width="{{ 120 }}" left-width="{{ 0 }}" name="示例" bind:open="onOpen" wx:for="{{datas}}" wx:for-item="item" wx:key='index'  >
    <!-- <van-cell-group>
      <van-cell title="单元格" value="内容" />
    </van-cell-group> -->
    <view class="DIV" bindtap="openTo" data-item='{{item}}'>
       <view class="left">
            <view  style="font-size:12px;font-weight:bold;height:20px">{{item.title}}</view>
            <view  style="font-size:25rpx;height:20px;color:red">{{item.scheduleStartTime}} - {{item.scheduleEndTime}}</view>
       </view>
        <view  class="right">
            <view style="height:20px"><van-icon name="{{forId(item.forId)}}"  size="12px"/></view>
            <view style="font-size:12px"><van-icon name="{{forId(item.forId)}}" size="10px" />22222</view>
       </view>
    </view>
    <view slot="right" class="van-swipe-cell__right">
    <van-button bind:click='openTo' data-item='{{item}}' type="primary">编辑</van-button>
    <van-button bind:click='delSchedule' data-item='{{item}}' type="warning">删除</van-button>
    </view>
  </van-swipe-cell>
